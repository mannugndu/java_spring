<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context-2.5.xsd">

	<bean class="com.naveen.vehicle.Car" id="car">
		<property name="modelName" value="Hyundai Creta"></property>
		<property name="modelYear" value="2019"></property>
		<property name="engine">
			<bean class="com.naveen.vehicle.Engine">
				<property name="name" value="V6"></property>
				<property name="weight" value="130.98"></property>
				<property name="size" value="SMALL"></property>
			</bean>
		</property>
	</bean>
</beans>